@model IEnumerable<ElYamanyDashboard.Models.Views.NotificationView>

@{
    ViewBag.Title = "Index";
}

<h2 style="text-align:center;color:white;border-radius:25px;background:#007bff">ريبورت اشعارات المستويات</h2>


     <div class="card">

         <!-- /.card-header -->
        <div class="card-body">
            <table id="example" class="table table-striped table-bordered" style="width:100%">
                <thead>
                    <tr>
                        
                        <th>
                            @Html.DisplayNameFor(model => model.Message)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.MessageAR)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.UserLevel)
                        </th>
                        <th>
                            @Html.DisplayName("تاريخ الارسال")
                        </th>
                    </tr>
                </thead>
                <tbody>

                    @foreach (var item in Model)
                    {
                    <tr>
                        
                        <td>
                            @Html.DisplayFor(modelItem => item.Message)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.MessageAR)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.UserLevel)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.CreationDatestring)
                        </td>

                    </tr>

                    }

                </tbody>

            </table>
        </div>
        <!-- /.card-body -->
    </div>

 
 