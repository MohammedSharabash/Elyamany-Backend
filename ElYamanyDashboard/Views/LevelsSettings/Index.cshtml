@model IEnumerable<ElYamanyDashboard.Models.LevelsSetting>

@{
    ViewBag.Title = "Index";
}


<h2 style="text-align:center;color:white;border-radius:25px;background:#007bff">المستويات</h2>

<div class="card">


    <!-- /.card-header -->
    <div class="card-body">
        <table id="example" class="table table-striped table-bordered" style="width:100%">
            <thead>
                <tr>
                    <th>
                        @Html.DisplayNameFor(model => model.LevelName)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.LevelLable)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.LevelFrom)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.LevelTo)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.LevelNumebr)
                    </th>
                    <th></th>
                </tr>
            </thead>
            <tbody>

                @foreach (var item in Model)
                {

                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.LevelName)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.LevelLable)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.LevelFrom)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.LevelTo)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.LevelNumebr)
                    </td>
                    <td>
                        @Html.ActionLink("تعديل", "Edit", new { id = item.Id })
                    </td>
                </tr>


                }

            </tbody>

        </table>
    </div>
    <!-- /.card-body -->
</div>
